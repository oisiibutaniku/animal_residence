<div class="contents row">
  <div class="container">
    <h2>プロフィールを編集</h2>
    <%= form_with(model:@user, local: true) do |f| %>

            プロフィール画像
        <span class="indispensable"></span>
      <div class="click-upload">
        <p>
          クリックしてファイルをアップロード
        </p>
        <%= f.file_field :image, id:"user-image" %>
    </div>
            <div class="field">
        <%= f.label :nickname %> <em>(6 characters maximum)</em><br />
        <%= f.text_field :nickname, autofocus: true, maxlength: "6" %>
      </div>
      <div class="field">
          テキスト
      <%= f.text_area :profile, rows: "5" %>
      </div>

      <div class="actions">
        <%= f.submit "送信" %>
      </div>
    <% end %>
  </div>
</div>